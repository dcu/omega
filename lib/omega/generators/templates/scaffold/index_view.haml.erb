#<%= underscore_name %>-index
  %h1
    <%= collection_name.titleize %>

  %ul
    %li(ng-repeat="<%= resource_name %> in <%= collection_name %>")
      %a(ng-href="<%= route %>/{{<%= resource_name %>._id}}")
        {{<%= resource_name %>.name}}

  %a(ng-href="<%= route %>/new")
    Create a new <%= resource_name %>

